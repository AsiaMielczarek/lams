<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.lamsfoundation.lams.tool.videoRecorder.model.VideoRecorderRating"
        table="tl_lavidr10_rating"
    >

        <id
            name="uid"
            column="uid"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-VideoRecorderRating.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="createDate"
            type="java.util.Date"
            update="true"
            insert="true"
            column="create_date"
        />

        <many-to-one
            name="createBy"
            class="org.lamsfoundation.lams.tool.videoRecorder.model.VideoRecorderUser"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="create_by"
        />

        <many-to-one
            name="recording"
            class="org.lamsfoundation.lams.tool.videoRecorder.model.VideoRecorderRecording"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="videoRecorder_recording_uid"
        />

        <property
            name="rating"
            type="float"
            update="true"
            insert="true"
            column="rating"
        />

        <many-to-one
            name="videoRecorderSession"
            class="org.lamsfoundation.lams.tool.videoRecorder.model.VideoRecorderSession"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="videoRecorder_session_uid"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-VideoRecorderRating.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
