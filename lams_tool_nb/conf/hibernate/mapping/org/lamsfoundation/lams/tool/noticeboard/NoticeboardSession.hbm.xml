<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.lamsfoundation.lams.tool.noticeboard.NoticeboardSession" 
    table="tl_lanb11_session"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="tl_lanb11_session"
    </meta>

    <id
        name="nbSessionId"
        type="java.lang.Long"
        column="nb_session_id"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Long"
            column="nb_session_id"


        </meta>
        <generator class="assigned" />
    </id>

    <property
        name="sessionStartDate"
        type="java.sql.Timestamp"
        column="session_start_date"
        length="19"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="session_start_date"
            length="19"
        </meta>    
    </property>
    <property
        name="sessionEndDate"
        type="java.sql.Timestamp"
        column="session_end_date"
        length="19"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="session_end_date"
            length="19"
        </meta>    
    </property>
    <property
        name="sessionStatus"
        type="java.lang.String"
        column="session_status"
        length="100"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="session_status"
            length="100"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TlLanb11Content -->
    <many-to-one
        name="nbContent"
        class="org.lamsfoundation.lams.tool.noticeboard.NoticeboardContent"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="nb_content_id"         
        </meta>
        <column name="nb_content_id" />
    </many-to-one>

</class>
</hibernate-mapping>
