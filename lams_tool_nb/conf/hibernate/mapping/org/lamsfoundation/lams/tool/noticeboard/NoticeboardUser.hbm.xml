<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.lamsfoundation.lams.tool.noticeboard.NoticeboardUser" 
    table="tl_lanb11_user"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="tl_lanb11_user"
    </meta>

    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="native"
            type="java.lang.Long"
            column="uid"


        </meta>
        <generator class="native" />
    </id>

    <property
        name="userId"
        type="java.lang.Long"
        column="user_id"
        not-null="true"
        length="20"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="user_id"
            length="20"
            not-null="true"
        </meta>    
    </property>
    <property
        name="username"
        type="java.lang.String"
        column="username"
        length="50"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="username"
            length="50"
        </meta>    
    </property>
    <property
        name="fullname"
        type="java.lang.String"
        column="fullname"
        length="50"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="fullname"
            length="50"
        </meta>    
    </property>
    <property
        name="userStatus"
        type="java.lang.String"
        column="user_status"
        length="50"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="user_status"
            length="50"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to TlLanb11Session -->
    <many-to-one
        name="nbSession"
        class="org.lamsfoundation.lams.tool.noticeboard.NoticeboardSession"
        not-null="true"
        cascade="none"
        outer-join="auto"
        update="true"
        insert="true"
        access="property"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="nb_session_uid"         
        </meta>
        <column name="nb_session_uid" />
    </many-to-one>

</class>
</hibernate-mapping>
