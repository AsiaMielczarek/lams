<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.lamsfoundation.lams.tool.wiki.persistence.WikiToolSession"
        table="tl_lawiki10_tool_session"
    >

        <id
            name="uid"
            column="uid"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-WikiToolSession.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <version
            name="version"
            column="version"
            type="int"
        />

        <property
            name="sessionEndDate"
            type="java.util.Date"
            update="true"
            insert="true"
            column="session_end_date"
        />

        <property
            name="sessionStartDate"
            type="java.util.Date"
            update="true"
            insert="true"
            column="session_start_date"
        />

        <property
            name="status"
            type="int"
            update="true"
            insert="true"
            column="status"
        />

        <many-to-one
            name="wiki"
            class="org.lamsfoundation.lams.tool.wiki.persistence.Wiki"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="wiki_uid"
        />

        <property
            name="sessionId"
            type="java.lang.Long"
            update="true"
            insert="true"
            column="session_id"
        />

        <property
            name="sessionName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="session_name"
            length="250"
        />

        <property
            name="markReleased"
            type="boolean"
            update="true"
            insert="true"
            column="mark_released"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-WikiToolSession.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
