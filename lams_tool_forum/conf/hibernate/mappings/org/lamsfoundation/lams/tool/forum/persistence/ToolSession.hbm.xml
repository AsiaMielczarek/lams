<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.lamsfoundation.lams.tool.forum.persistence.ToolSession"
        table="tl_lafrum11_tool_session"
    >

        <id
            name="uuid"
            column="uuid"
            type="java.lang.Long"
        >
            <generator class="identity">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ToolSession.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="sessionEndDate"
            type="java.util.Date"
            update="true"
            insert="true"
            column="sessionEndDate"
        />

        <property
            name="sessionStartDate"
            type="java.util.Date"
            update="true"
            insert="true"
            column="sessionStartDate"
        />

        <property
            name="status"
            type="int"
            update="true"
            insert="true"
            column="status"
        />

        <many-to-one
            name="forum"
            class="org.lamsfoundation.lams.tool.forum.persistence.Forum"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="uuid"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ToolSession.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
