<%-- Error Messages --%>
<form:errors path="*" />

<%-- Success Messages -
<logic:messagesPresent message="true">
	<lams:Alert id="message" close="false" type="info">
		<html:messages id="message" message="true">
			<c:out value="${message}" escapeXml="false" />
		</html:messages>
	</lams:Alert>
</logic:messagesPresent>
--%>