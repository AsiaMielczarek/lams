<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.lamsfoundation.lams.tool.qa.ResponseRating"
        table="tl_laqa11_response_rating"
    >

        <id
            name="uid"
            column="uid"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ResponseRating.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="user"
            class="org.lamsfoundation.lams.tool.qa.QaQueUsr"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="user_id"
        />

        <property
            name="rating"
            type="float"
            update="true"
            insert="true"
            column="rating"
        />

        <many-to-one
            name="response"
            class="org.lamsfoundation.lams.tool.qa.QaUsrResp"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="response_id"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ResponseRating.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
