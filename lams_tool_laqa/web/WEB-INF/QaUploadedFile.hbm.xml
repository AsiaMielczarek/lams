<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.lamsfoundation.lams.tool.qa.QaUploadedFile" 
    table="tl_laqa11_uploadedFile"
>

    <id
        name="submissionId"
        type="java.lang.Long"
        column="submissionId"
    >
	    <generator class="increment"/>
    </id>


    <property
        name="uuid"
        type="java.lang.String"
        access="property"
        column="uuid"
        length="255"
    />

   <property
            name="fileOnline"
            type="boolean"
            access="property"
            column="isOnline_File"
   />

   <property
            name="fileName"
            type="java.lang.String"
            access="property"
            column="fileName"
   />
   
       <!--  property
        name="qaContentId"
        type="java.lang.Long"
        update="false"
        insert="false"
        access="property"
        column="qa_content_id"
        length="20"
    / -->
   

   <many-to-one
        name="qaContent"
        class="org.lamsfoundation.lams.tool.qa.QaContent"
        not-null="true"        
        cascade="none"
        outer-join="auto"
        update="true"
        insert="true"
        access="property"
    >
        <column
            name="qa_content_id" 
        />
    </many-to-one>

</class>
</hibernate-mapping>
