import org.lamsfoundation.lams.common.ui.*

/**
* Point class - holds x and y value as pixels.  
*  
* @author  DI
* 
*/

class Point  {
    
    public var x:Number;
    public var y:Number;
    
    /**
    * Constructor
    */
    function Point(x:Number,y:Number){
        this.x = x;
        this.y = y;
    }
}