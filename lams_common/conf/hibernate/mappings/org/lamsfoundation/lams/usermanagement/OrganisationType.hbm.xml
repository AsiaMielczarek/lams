<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.lamsfoundation.lams.usermanagement.OrganisationType" 
    table="lams_organisation_type"
>

	<cache usage="read-only" />
    
    
	
	

    <id
        name="organisationTypeId"
        type="java.lang.Integer"
        column="organisation_type_id"
    >
        
        
        <generator class="increment" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="name"
        not-null="true"
        unique="true"
        length="64"
    >
            
    </property>
    <property
        name="description"
        type="java.lang.String"
        column="description"
        not-null="true"
        length="255"
    >
            
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to Organisation -->
    <set
        name="organisations"
        lazy="true"
        inverse="true"
		cascade="none"
    >
    <cache usage="transactional" />

        
        <key>
            <column name="organisation_type_id" />
        </key>
        <one-to-many 
            class="org.lamsfoundation.lams.usermanagement.Organisation"
        />
    </set>

</class>
</hibernate-mapping>
