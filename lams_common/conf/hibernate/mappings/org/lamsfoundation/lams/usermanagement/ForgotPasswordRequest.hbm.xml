<class 
    name="org.lamsfoundation.lams.usermanagement.ForgotPasswordRequest" 
    table="lams_password_request"
>

		<cache usage="transactional" />
    
    
    

    

    <id
        name="requestId"
        type="java.lang.Integer"
        column="request_id"
    >
        
        <generator class="native" />
    </id>

    <property
        name="userId"
        type="java.lang.Integer"
        column="user_id"
        not-null="true"
    >
            
    </property>
    
    <property
        name="requestKey"
        type="java.lang.String"
        column="request_key"
        not-null="true"
        unique="true"
        length="36"
    >
        
            
    </property>
    
    
    <property
        name="requestDate"
        type="java.sql.Timestamp"
        column="request_date"
        not-null="true"
        length="19"
    >
            
    </property>
    
    
</class>

</hibernate-mapping>