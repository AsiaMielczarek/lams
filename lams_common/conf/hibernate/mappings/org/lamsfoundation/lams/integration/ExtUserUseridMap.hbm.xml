<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="org.lamsfoundation.lams.integration.ExtUserUseridMap" 
    table="lams_ext_user_userid_map"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="lams_ext_user_userid_map"
    </meta>
	
    <id
        name="sid"
        type="java.lang.Integer"
        column="sid"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="native"
            type="java.lang.Integer"
            column="sid"


        </meta>
        <meta attribute="use-in-equals">true</meta>
        <generator class="native" />
    </id>

    <property
        name="foreignUsername"
        type="java.lang.String"
        column="external_username"
        not-null="true"
        length="250"
    >
        <meta attribute="use-in-tostring">true</meta>
        <meta attribute="field-description">
           @hibernate.property
            column="external_username"
            length="250"
            not-null="true"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional many-to-one association to User -->
    <many-to-one
        name="user"
        class="org.lamsfoundation.lams.usermanagement.User"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="user_id"         
        </meta>
        <column name="user_id" />
    </many-to-one>
    <!-- bi-directional many-to-one association to ExtServerOrgMap -->
    <many-to-one
        name="extServerOrgMap"
        class="org.lamsfoundation.lams.integration.ExtServerOrgMap"
        not-null="true"
    >
        <meta attribute="field-description">
           @hibernate.many-to-one
            not-null="true"
           @hibernate.column name="ext_server_org_map_id"         
        </meta>
        <column name="ext_server_org_map_id" />
    </many-to-one>

</class>
</hibernate-mapping>
