<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.lamsinternational.lams.learningdesign.LearningLibrary" 
    table="lams_learning_library"
>
 
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="learningLibraryId"
        type="java.lang.Long"
        column="learning_library_id"
    >
 
        <generator class="identity" />
    </id>

    <property
        name="description"
        type="java.lang.String"
        column="description"
        length="65535"
    >
 
    </property>
    <property
        name="title"
        type="java.lang.String"
        column="title"
        length="255"
    >

    </property>
    <property
        name="createDateTime"
        type="java.sql.Timestamp"
        column="create_date_time"
        not-null="true"
        length="19"
    >
 
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to Activity -->
    <list
        name="activities"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        
        <key>
            <column name="learning_library_id" />
        </key>
        <one-to-many 
            class="com.lamsinternational.lams.learningdesign.Activity"
        />
        <index column="order_id"/>
    </list>

</class>
</hibernate-mapping>
