<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.lamsinternational.lams.learningdesign.GroupingType" 
    table="lams_grouping_type"
>
    <meta attribute="class-description" inherit="false">
       @hibernate.class
        table="lams_grouping_type"
    </meta>
    <meta attribute="implement-equals" inherit="false">true</meta>

    <id
        name="groupingTypeId"
        type="java.lang.Integer"
        column="grouping_type_id"
    >
        <meta attribute="field-description">
           @hibernate.id
            generator-class="assigned"
            type="java.lang.Integer"
            column="grouping_type_id"


        </meta>
        <generator class="assigned" />
    </id>

    <property
        name="description"
        type="java.lang.String"
        column="description"
        not-null="true"
        length="128"
    >
        <meta attribute="field-description">
           @hibernate.property
            column="description"
            length="128"
            not-null="true"
        </meta>    
    </property>

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to Grouping -->
    <set
        name="groupings"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <meta attribute="field-description">
           @hibernate.set
            lazy="true"
            inverse="true"
            cascade="none"

           @hibernate.collection-key
            column="grouping_type_id"

           @hibernate.collection-one-to-many
            class="com.lamsinternational.lams.learningdesign.Grouping"
        </meta>
        <key>
            <column name="grouping_type_id" />
        </key>
        <one-to-many 
            class="com.lamsinternational.lams.learningdesign.Grouping"
        />
    </set>

</class>
</hibernate-mapping>
