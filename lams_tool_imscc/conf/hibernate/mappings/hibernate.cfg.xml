<?xml version="1.0"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration
DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory>
		<property name="connection.username">lams</property>
		<property name="connection.password">lamsdemo</property>
		<property name="connection.url">jdbc:mysql://localhost/lams</property>
		<property name="connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="dialect">org.hibernate.dialect.MySQLDialect</property>
		<mapping commonCartridge="org/lamsfoundation/lams/learningdesign/BranchCondition.hbm.xml"/>
	</session-factory>
</hibernate-configuration>
