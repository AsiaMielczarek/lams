<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.eucm.lams.tool.eadventure.model.EadventureExpression"
        table="tl_eueadv10_condition_expression"
    >

        <id
            name="uid"
            column="uid"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-EadventureExpression.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="firstOp"
            class="org.eucm.lams.tool.eadventure.model.EadventureParam"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="first_op"
        />

        <property
            name="valueIntroduced"
            type="java.lang.String"
            update="true"
            insert="true"
            column="value_introduced"
        />

        <many-to-one
            name="varIntroduced"
            class="org.eucm.lams.tool.eadventure.model.EadventureParam"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="var_introduced"
        />

        <property
            name="expresionOp"
            type="java.lang.String"
            update="true"
            insert="true"
            column="expresion_op"
        />

        <property
            name="sequenceId"
            type="int"
            update="true"
            insert="true"
            column="sequence_id"
        />

        <many-to-one
            name="condition"
            class="org.eucm.lams.tool.eadventure.model.EadventureCondition"
            cascade="save-update"
            outer-join="auto"
            update="true"
            insert="true"
            column="condition_uid"
        />

        <property
            name="nextOp"
            type="java.lang.String"
            update="true"
            insert="true"
            column="next_op"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-EadventureExpression.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
