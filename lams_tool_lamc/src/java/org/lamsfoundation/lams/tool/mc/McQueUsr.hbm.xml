<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.lamsfoundation.lams.tool.mc.McQueUsr" 
    table="tl_lamc11_que_usr"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
		    <generator class="native"/>
    </id>

	<property
        name="queUsrId"
        type="java.lang.Long"
        column="que_usr_id"
        not-null="true"
        length="20"
    >
    </property>


    <property
        name="username"
        type="java.lang.String"
        column="username"
        length="100"
    >
    </property>
    <property
        name="fullname"
        type="java.lang.String"
        column="fullname"
        length="100"
    >
    </property>

    <property
        name="mcSessionId"
        type="java.lang.Long"
        column="mc_session_id"
        update="false"
        insert="false"
    >
    </property>
    
    <many-to-one
        name="mcSession"
        class="org.lamsfoundation.lams.tool.mc.McSession"
        cascade="none"
        outer-join="auto"
        update="true"
        insert="true"
        access="property"
    >
        <column name="mc_session_id" />
    </many-to-one>

</class>
</hibernate-mapping>
