<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.lamsfoundation.lams.tool.mc.McUsrAttempt" 
    table="tl_lamc11_usr_attempt"
>
    <id
        name="uid"
        type="java.lang.Long"
        column="uid"
    >
        <generator class="native" />
    </id>

    <property
        name="attemptId"
        type="java.lang.Long"
        column="attempt_id"
        not-null="true"
        length="20"
    >
    </property>
    
    <property
        name="attemptTime"
        type="java.util.Date"
        update="true"
        insert="true"
        access="property"
        column="attempt_time"
        length="20"
    >
    </property>
    
    <property
        name="timeZone"
        type="java.lang.String"
        update="true"
        insert="true"
        access="property"
        column="time_zone"
        length="255"
    >
    </property>

    <many-to-one
        name="mcQueContent"
        class="org.lamsfoundation.lams.tool.mc.McQueContent"
   	    cascade="none"
	    outer-join="auto"
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="mc_que_content_id" />
    </many-to-one>
    
    <many-to-one
        name="mcQueUsr"
        class="org.lamsfoundation.lams.tool.mc.McQueUsr"
	    cascade="none"
	    outer-join="auto"
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="que_usr_id" />
    </many-to-one>
    
    <many-to-one
        name="mcOptionsContent"
        class="org.lamsfoundation.lams.tool.mc.McOptsContent"
	    cascade="none"
	    outer-join="auto"
	    update="true"
	    insert="true"
	    access="property"
    >
        <column name="mc_que_option_id" />
    </many-to-one>

</class>
</hibernate-mapping>
